#exit
uci set network.wlan=interface
uci set network.wlan.proto=dhcp
uci set network.wlan.macaddr='50:d4:f7:33:15:8d'
uci set network.wlan2=interface
uci set network.wlan2.proto=dhcp
uci set network.wlan2.macaddr='50:d4:f7:33:15:8e'
uci set wireless.radio0.disabled=0
uci set wireless.default_radio0.mode=sta
uci set wireless.default_radio0.ssid='PB1202'
uci set wireless.default_radio0.key='hakuna_matata'
uci set wireless.default_radio0.encryption='psk2'
uci set wireless.default_radio0.network=wlan
#uci set wireless.radio1.disabled=0
uci set wireless.default_radio1.mode=sta
uci set wireless.default_radio1.ssid='PB1202'
uci set wireless.default_radio1.key='hakuna_matata'
uci set wireless.default_radio1.encryption='psk2'
uci set wireless.default_radio1.network=wlan2

uci add_list firewall.@zone[0].network='wlan'
uci add_list firewall.@zone[0].network='wlan2'

