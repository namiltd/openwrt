/dts-v1/;

#include "mt7620a_tplink_archer.dtsi"
#include <dt-bindings/interrupt-controller/irq.h>

/ {
	compatible = "tplink,archer-c5-v4", "ralink,mt7620a-soc";
	model = "TP-Link Archer C5 v4";

	chosen {
		bootargs = "console=ttyUSB0,9600";
	};

	aliases {
		led-boot = &led_power;
		led-failsafe = &led_power;
		led-running = &led_power;
		led-upgrade = &led_power;
		label-mac-device = &ethernet;
	};

	leds {
		compatible = "gpio-leds";

		lan {
			label = "green:lan";
			gpios = <&gpio0 8 GPIO_ACTIVE_HIGH>;
		};

		led_power: power {
			label = "green:power";
			gpios = <&gpio2 2 GPIO_ACTIVE_LOW>;
		};

		usb {
			label = "green:usb";
			gpios = <&gpio0 7 GPIO_ACTIVE_LOW>;
			trigger-sources = <&ohci_port1>, <&ehci_port1>;
			linux,default-trigger = "usbport";
		};

		wan {
			label = "green:wan";
			gpios = <&gpio0 10 GPIO_ACTIVE_LOW>;
		};

		wan_orange {
			label = "orange:wan";
			gpios = <&gpio0 9 GPIO_ACTIVE_LOW>;
		};

		wlan2g {
			label = "green:wlan2g";
			gpios = <&gpio0 14 GPIO_ACTIVE_LOW>;
			linux,default-trigger = "phy1tpt";
		};

		wlan5g {
			label = "green:wlan5g";
			gpios = <&gpio0 12 GPIO_ACTIVE_LOW>;
			linux,default-trigger = "phy0tpt";
		};

		wps {
			label = "green:wps";
			gpios = <&gpio0 1 GPIO_ACTIVE_LOW>;
		};
	};
};

&gpio0 {
	interrupt-controller;
	#interrupt-cells = <2>;
};

&state_default {
	gpio {
		groups = "i2c", "uartf", "ephy", "rgmii2";
		function = "gpio";
	};
};

&ethernet {
	pinctrl-0 = <&rgmii1_pins &mdio_pins>;

	/* Internal switch is used as transparent switch connecting
	   its port6 (CPU) to external switch through port5 */
	/delete-property/ mediatek,portmap;

	/* Currrent realtek DSA driver forces forwarding on CPU */
	mediatek,mac-learn-disable;

	port@5 {
		status = "okay";
		mediatek,fixed-link = <1000 1 1 1>;
		phy-mode = "rgmii";
	};

	mdio0: mdio-bus {
		status = "okay";
	};
};

&mdio0 {
	switch1@29 {

		compatible = "realtek,rtl8365mb";
		reg = <29>;

		reset-gpios = <&gpio2 20 GPIO_ACTIVE_LOW>;

	        switch_intc: interrupt-controller {
			interrupt-parent = <&gpio0>;
			interrupts = <11 IRQ_TYPE_EDGE_FALLING>;
			interrupt-controller;
			#address-cells = <0>;
			#interrupt-cells = <1>;
	        };

		mdio {
			#address-cells = <1>;
			#size-cells = <0>;

			phy0: ethernet-phy@0 {
				reg = <0>;
				interrupt-parent = <&switch_intc>;
				interrupts = <0>;
			};
			phy1: ethernet-phy@1 {
				reg = <1>;
				interrupt-parent = <&switch_intc>;
				interrupts = <1>;
			};
			phy2: ethernet-phy@2 {
				reg = <2>;
				interrupt-parent = <&switch_intc>;
				interrupts = <2>;
			};
			phy3: ethernet-phy@3 {
				reg = <3>;
				interrupt-parent = <&switch_intc>;
				interrupts = <3>;
			};
			phy4: ethernet-phy@4 {
				reg = <4>;
				interrupt-parent = <&switch_intc>;
				interrupts = <4>;
			};
		};

		ports {
			#address-cells = <1>;
			#size-cells = <0>;
			reg = <0>;

			port@0 {
				reg = <0>;
				label = "lan4";
				phy-handle = <&phy0>;
			};

			port@1 {
				reg = <1>;
				label = "lan3";
				phy-handle = <&phy1>;
			};

			port@2 {
				reg = <2>;
				label = "lan2";
				phy-handle = <&phy2>;
			};

			port@3 {
				reg = <3>;
				label = "lan1";
				phy-handle = <&phy3>;
			};

			port@4 {
				reg = <4>;
				label = "wan";
				nvmem-cells = <&macaddr_rom_f100>;
				nvmem-cell-names = "mac-address";
				mac-address-increment = <(1)>;
				phy-handle = <&phy4>;
			};

			port@7 {
				reg = <7>;
				ethernet = <&ethernet>;
				phy-mode = "rgmii";
				tx-internal-delay-ps = <2000>;
				rx-internal-delay-ps = <0>;

				dsa-tag-protocol = "rtl8_4";

				fixed-link {
					speed = <1000>;
					full-duplex;
				};
			};
		};
	};
};

&wmac {
	nvmem-cells = <&macaddr_rom_f100>;
	nvmem-cell-names = "mac-address";
};

&wifi {
	nvmem-cells = <&macaddr_rom_f100>;
	nvmem-cell-names = "mac-address";
	mac-address-increment = <(2)>;
};
